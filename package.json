{
  "name": "poketto-offline-client",
  "version": "1.0.0",
  "description": "Browser-based personal accounting app",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --env ENVIRONMENT=development --config webpack.config.dev.js",
    "prebuild": "npm run test",
    "build": "webpack --env ENVIRONMENT=production --config webpack.config.prod.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "prettier": "prettier --write ./src/**/*.{ts,tsx,js,json}",
    "format": "npm run prettier && npm run lint:fix",
    "pretest": "npm run format",
    "test": "jest --passWithNoTests --coverage"
  },
  "author": "Joe-Mar Gonzales",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "auto-prefixer": "^0.4.2",
    "babel-loader": "^9.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/nprogress": "^0.2.0",
    "clsx": "^1.2.1",
    "gh-pages": "^5.0.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0"
  }
}
